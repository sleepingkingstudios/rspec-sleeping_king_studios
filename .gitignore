# .gitignore

# Ignore local configuration files.
/.rspec
/.ruby-gemset
/.ruby-version

# Ignore compiled gem files.
*.gem
.bundle

# Ignore Gemfile locks.
Gemfile.lock
gemfiles/*.lock

# Ignore local and generated documentation.
/doc
.yardoc
.yardopts

# Ignore temporary files.
/.byebug_history
/.rubocop.yml
/.rubocop_todo.yml
/tmp

# Vim Swap files
*.swp
